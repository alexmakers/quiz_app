<button class='start-quiz'>Start Quiz</button> <br>

<div class='multiplayer'>

  <h4 class='result'></h4>
  <h5 class='my-answer'></h5>

  <div class='timer'>
  </div>

  <% @quiz.questions.each do |question| %>

    <div class='question'>

      <h4><%= question.query %></h4>

      <% question.answers.each do |answer| %>

        <button data-question-id="<%= question.id %>" data-id="<%= answer.id %>">
          <%= answer.response %>
        </button>

      <% end %>

    </div>

  <% end %>
</div>